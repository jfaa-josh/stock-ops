PYTHON_VERSION=3.13
PREFECT_VERSION=3.4.10

# Provider tokens
EODHD_API_TOKEN=__PUT_YOUR_TOKEN_HERE__

# Required for production deployments: >>>>>>>>>>>>>>>>>>>>>>
# Domain used for nginx + certbot (public DNS name)
PRODUCTION_DOMAIN=your.production.domain

# Let's Encrypt account email for cert issuance/renewal notices
LETSENCRYPT_EMAIL=you@example.com

# How often nginx reloads to pick up renewed certs (seconds)
NGINX_RELOAD_INTERVAL_SECONDS=43200
# >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>
